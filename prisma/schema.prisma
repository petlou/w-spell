generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Character {
  id        String   @id @default(uuid())
  name      String   @unique
  createdAt DateTime @default(now())

  @@map("characters")
}
